<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:cube="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.catalinjurjiu.demoanimcubeandroid.MainActivity">

    <com.catalinjurjiu.animcubeandroid.AnimCube
        android:id="@+id/animcube"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        cube:backFacesDistance="4"
        cube:backgroundColor="#d0d0d0"
        cube:debuggable="true"
        cube:editable="true"
        cube:moves="R2' U M U' R2' U M' U'"
        cube:singleRotationSpeed="30"/>

</FrameLayout>
